
public class Person {
	public String name = null;
	public int age = 0;
	public String phoneNumber = null;
	public String address = null;

	public Person disp() {
		Person p = new Person();
		p.name = this.name;
		p.age = this.age;
		p.phoneNumber = this.phoneNumber;
		p.address = this.address;

		return p;
	}
}
